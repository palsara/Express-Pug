extends layout

block content
  .container
    .row
      .col-xs-10.col-xs-offset-1
        table.table.table-striped
          thead
            tr
              th #
              th name
              th email
              th job
          tbody
            each user, index in users
              tr
                td #{index+1}
                td #{user.name}
                td #{user.email}
                td #{user.job}
            else 
              tr
                td(colspan='4') no users yet
    .row
      ul
        -let iterator=0;
        while iterator<10
          li= iterator++
